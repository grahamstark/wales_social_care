<!DOCTYPE database PUBLIC "http://virtual-worlds.biz/Mill"  "http://www.virtual-worlds.biz/dtds/mill.dtd">
<database name="wsc_db">


        <adaTypePackage name="WSC_Enums"/>
        <adaTypePackage name="Base_Types"/>

        <table name="user_type" description="user">
                <column name="username" primaryKey="true" type="CHAR" size="32" description="Username" />
                <column name="password" type="CHAR" size="60" description="password" />
                <column name="title" type="CHAR" size="60" description="title" />
                <column name="description" type="CHAR" size="60" description="description" />
                <column name="email" type="CHAR" size="60" description="email" />
                <column name="work_dir" type="CHAR" size="60" description="work_dir" />
                <column name="utype" type="ENUM" values='anon registered expert admin invalid deleted' description="utype" />
                <column name="lang" type="ENUM"  values='en' description="lang" />
                <column name="preferences" type="CHAR" size="60" description="preferences" />
                <column name="last_used" type="TIMESTAMP" description="last_used" />
        </table>
        
        <table name='dataset'>
                <column name='name' primaryKey="true" type="CHAR" size="32" description="Name" />
                <column name="creator" type="CHAR" size="32" description="Username of creator" />
                <column name="title" type="CHAR" size="60" description="title" />
                <column name="run_id"  type="INTEGER"  /> <!-- should really be a foreign key but don't see how" -->
                
                <foreign-key foreignTable="user_type" onDelete='SETNULL' onUpdate='CASCADE' >
                        <reference foreign="username" local="creator" />
                </foreign-key>
        </table>

        <table name='run'>
       
                <column name="run_id"  primaryKey="true" type="INTEGER"  />
                <column name="username" primaryKey="true" type="CHAR" size="32" description="Username" />
                <column name="comparison_username" type="CHAR" size="60" description="comparison_run_id" />
                <column name="comparison_run_id" type="INTEGER" description="comparison_run_id" />
                
                <column name="title" type="CHAR" size="60" description="title" />
                <column name="use_random_threshold" type="BOOLEAN"  />
                <column name="num_iterations" type="INTEGER" default='1' />
                <column name="interest_rate_pct" type="REAL" size="60" description="interest_rate_pct" />
                <column name="real_terms" type='BOOLEAN'  />
                <column name="is_null_settings" type="BOOLEAN" default='0' /> <!-- not bug in parser means default 'true' won't work -->
                      
                <column name="working_root" type="CHAR" size="120" description="working_root" />
                      
                <column name="users_directory" type="CHAR" size="120" description="users_directory" />
                <column name="output_directory" type="CHAR" size="120" description="output_directory" />
                <column name="dir_separator" type="CHAR" size="1" description="dir_separator" />
                <column name="session_id" type="CHAR" size="60" description="session_id" />
                      
                <column name="dataset_name" type="CHAR" size='32' description="Dataset Name" />
                <column name="default_run_dir_id" type="INTEGER" description="default_run_dir_id" />
                      
                <column name="start_year" type="INTEGER" description="start_year" />
                <column name="end_year" type="INTEGER" description="end_year" />
                <column name="weighting_function" type="ENUM" values=
                             'chi_square 
                              d_and_s_type_a 
                              d_and_s_type_b
                              constrained_chi_square 
                              d_and_s_constrained' 
                        description="weighting_function" default='3'/>
                <column name="weighting_lower_bound" type="REAL" description="weighting_lower_bound" default='0.2'/>
                <column name="weighting_upper_bound" type="REAL" description="weighting_upper_bound" default='2.0' />
                <column name="status" type="ENUM" values='edited displayed neither' description="Run Status" />
                <column name="type_of_run" type="ENUM" values='simulation data_creation' default='0' description="Run Status" />
                <column name="do_reweighting" type="BOOLEAN" default='0' description="Do Reweighting Step" />
                
                <foreign-key foreignTable="user_type"  onUpdate='CASCADE' onDelete='CASCADE'>
                        <reference foreign="username" local="username"  />
                </foreign-key>
                <foreign-key foreignTable="dataset"  onUpdate='CASCADE' onDelete='SETNULL'>
                        <reference foreign="name" local="dataset_name"  />
                </foreign-key>
                
      </table>
      
      <table name='state'>
                <column name="username" primaryKey="true" type="CHAR" size="32" description="Username" />
                <column name="run_id"  primaryKey="true" type="INTEGER"  />
                <column name="household" type="INTEGER" default='0' />
                <column name="other_counter" type="INTEGER" default='0' />
                <column name="other_counter2" type="INTEGER" default='0' />
                <column name="other_counter3" type="INTEGER" default='0' />
                <column name="other_counter4" type="INTEGER" default='0' />
                <column name="year" type="INTEGER" default='0' />
                <column name="phase" type="ENUM" values='not_started queued run_starting pre_calculations running generating_output complete'  />
                <column name="health" type="ENUM" values='normal aborted in_error queued'  />
                <column name="error_code" type="INTEGER" default='0' />
                <column name="read_error" type="BOOLEAN" default='0' />
                <column name="session_id" type="CHAR" size='120' description='session id from which this run was stated, if any' />
                
                <foreign-key foreignTable="run">
                        <reference foreign="username" local="username"/>
                        <reference foreign="run_id" local="run_id"/>
                </foreign-key>
      </table>
      
      
        <table name='key_value_parameter'>
                <column name="username" primaryKey="true" type="CHAR" size="32" description="Username" />
